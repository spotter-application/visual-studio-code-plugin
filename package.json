{
  "name": "@spotter-app/visual-studio-plugin-plugin",
  "version": "0.0.1",
  "private": false,
  "type": "commonjs",
  "scripts": {
    "start": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "build": "tsc && pkg package.json && cd dist && zip plugin.zip visual-studio-plugin-plugin"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@spotter-app/plugin": "^2.0.0-beta.63",
    "@types/node": "^17.0.29",
    "pkg": "^5.6.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.4"
  },
  "bin": "dist/index.js",
  "pkg": {
    "scripts": "dist/*.js",
    "outputPath": "dist",
    "targets": [
      "node17-macos-x64"
    ]
  }
}
